
<script src="/js/dom/consultation.js" defer></script>

    <%- include('./component/head') %>
    <body>
      <div class="wrapper">
        <%- include('./component/sidebarUser') %>
  
  
            <%- include('./component/navbar') %>
  
            <div class="container">
              <div class="page-inner">
                <div class="d-flex align-items-left align-items-md-center flex-column flex-md-row pt-2 pb-4">
                  <form id="consultationForm" class="w-100">
                    <div class="row">
                      <div class="col-md-6">
                        <div class="form-group">
                          <label for="namaHewan">Nama Hewan</label>
                          <input
                            type="text"
                            class="form-control"
                            id="namaHewan"
                            name="namaHewan"
                            placeholder="Masukan nama hewan"
                          />
                        </div>
                      </div>
                      <div class="col-md-6">
                        <div class="form-group">
                          <label for="umurHewan">Umur hewan</label>
                          <input
                            type="number"
                            class="form-control"
                            id="umurHewan"
                            name="umurHewan"
                            placeholder="Masukan umur hewan"
                          />
                        </div>
                      </div>
                    </div>
        
                    <div class="row">
                      <div class="col-md-6">
                        <div class="form-group">
                          <label for="jenisKelamin">Jenis Kelamin</label><br />
                          <div class="d-flex">
                            <div class="form-check mr-3">
                              <input
                                class="form-check-input"
                                type="radio"
                                name="jenisKelamin"
                                id="jenisKelaminJantan"
                                value="Jantan"
                              />
                              <label class="form-check-label" for="jenisKelaminJantan">
                                Jantan
                              </label>
                            </div>
                            <div class="form-check">
                              <input
                                class="form-check-input"
                                type="radio"
                                name="jenisKelamin"
                                id="jenisKelaminBetina"
                                value="Betina"
                                checked
                              />
                              <label class="form-check-label" for="jenisKelaminBetina">
                                Betina
                              </label>
                            </div>
                          </div>
                        </div>
                      </div>
                      <div class="col-md-6">
                        <div class="form-group">
                          <label for="jenisHewan">Jenis Hewan</label>
                          <input
                            type="text"
                            class="form-control"
                            id="jenisHewan"
                            name="jenisHewan"
                            placeholder="Masukan Jenis Hewan"
                          />
                        </div>
                      </div>
                    </div>
        
                    <div class="form-group">
                      <label for="keluhan">Keluhan</label>
                      <textarea
                        class="form-control"
                        id="keluhan"
                        name="keluhan"
                        rows="5"
                      ></textarea>
                    </div>
        
                    <div class="card-action">
                      <button type="submit" class="btn btn-success mr-2" id ="alert_demo_3_3">Submit</button>
                      <button type="button" id="cancelButton" class="btn btn-danger">Cancel</button>
                    </div>
                    <div id="message"></div>
                    <div id="sweetalert"></div>
                  </form>
                </div>
              </div>
        
  
             <%- include('./component/footer') %>
  
      </div>
      <%- include('./component/scirpt') %>
  
   
  